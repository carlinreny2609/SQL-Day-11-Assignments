use  shopping;

INSERT INTO `shopping`.`products`
(`Sl_no`,
`Product_Name`,
`Net_weight_Grams`,
`Price_Rs`,
`No_Of_Products`)
VALUES
(11,'Moongdhall','500','32',20),
(12,'Soya','500','22',20);


select * from products ;

delimiter $$
create function Minumum_Price( Net_weight int(15))

returns int
deterministic
begin

declare min_price int;
select min(price_Rs)
into min_price 
from products 
where Net_weight_Grams = Net_weight; 
return min_price;
end $$


delimiter ;

select Minumum_Price('500') as min_price;
